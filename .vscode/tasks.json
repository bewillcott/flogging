{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "doc",
			"args": [
				"--no-deps",
				"--document-private-items"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo doc (dev)"
		},
		{
			"type": "cargo",
			"command": "test",
			"env": {
				"RUST_BACKTRACE": "0"
			},
			"problemMatcher": [
				"$rustc"
			],
			"group": "test",
			"label": "rust: cargo test"
		}
	]
}